{
  "name": "vue2.0-webpack",
  "version": "0.0.1",
  "description": "this is vue2 webpack project repository.",
  "main": "index.js",
  "scripts": {
    "clean": "del dist",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --inline --hot --config build/webpack.dev.conf.js",
