    "vue-router": "^2.0.0",
    "vuex": "^2.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.16.0",
    "browser-sync": "^2.17.0",
    "cross-env": "^3.0.0",
